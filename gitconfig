  name = Andrew O'Brien
[alias]
  co = checkout
  st = status
  br = branch
  w = whatchanged
  ls = log --graph --color --pretty=\"format:%C(yellow)%h%Creset%C(green)%d%Creset %s %Cblue[%aN]%Creset %C(magenta){%cr}%Creset\"
  la = log --graph --color --pretty=\"format:%C(yellow)%h%Creset%C(green)%d%Creset %s %Cblue[%aN]%Creset %C(magenta){%cr}%Creset\" --all
[apply]
  whitespace = nowarn
[color]
  ui = auto
  pager = true
  interactive = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = green
    changed = yellow
    untracked = cyan
[merge]
  summary = true
[push]
  default = tracking
[core]
	excludesfile = /Users/aobrien/.gitignore
